						"type": "text/javascript",
						"packages": {}
					}
				},
				{
					"listen": "prerequest",
					"script": {
						"exec": [
							"const crypto = require('crypto-js');\r",
							"\r",
							"const header = {\r",
							"    \"alg\": \"HS256\",\r",
							"    \"typ\": \"JWT\"\r",
							"};\r",
							"\r",
							"const now = Math.floor(Date.now() / 1000);  // ✅ Generate time in UTC\r",
							"\r",
							"const payload = {\r",
							"    \"token_type\": \"access\",\r",
							"    \"exp\": now + 900,  // ✅ Expires after 15 minutes (900 seconds)\r",
							"    \"iat\": now,\r",
							"    \"jti\": crypto.lib.WordArray.random(16).toString(crypto.enc.Hex),  // Unique Token ID\r",
							"    \"user_id\": 1\r",
							"};\r",
							"\r",
							"// Function to convert JSON to base64url format\r",
							"function base64url(source) {\r",
							"    return Buffer.from(JSON.stringify(source), 'utf8')\r",
							"        .toString('base64')\r",
							"        .replace(/=/g, '')\r",
							"        .replace(/\\+/g, '-')\r",
							"        .replace(/\\//g, '_');\r",
							"}\r",
							"\r",
							"// Encode header and payload\r",
							"const headerBase64 = base64url(header);\r",
							"const payloadBase64 = base64url(payload);\r",
							"\r",
							"// Use UTF-8 encoding for HMAC Signature\r",
							"const secret = \"django-insecure-(c!9%3d508b+zcuh)h+!^4khe!(50dzv&2u3b7q8eq1x)ns0k2\";\r",
							"const secretKey = crypto.enc.Utf8.parse(secret);\r",
							"const signature = crypto.HmacSHA256(headerBase64 + \".\" + payloadBase64, secretKey);\r",
							"const signatureBase64 = base64url(signature.toString(crypto.enc.Base64));\r",
							"\r",
							"// Construct JWT token\r",
							"const jwtToken = `${headerBase64}.${payloadBase64}.${signatureBase64}`;\r",
							"\r",
							"// Set the token in Postman environment\r",
							"pm.environment.set(\"jwt_token\", jwtToken);\r",
							"\r",
							"console.log(\"Generated JWT Token:\", jwtToken);\r",
							""
						],
						"type": "text/javascript",
						"packages": {}
					}
				}
			],
